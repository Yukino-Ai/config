{
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "update.showReleaseNotes": false,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "GitLive.Issue tracker integration": "Disabled",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.checkJs": true,
  "svelte.enable-ts-plugin": true,
  "editor.formatOnSave": true,
  "editor.bracketPairColorization.enabled": true,
  "workbench.editorAssociations": {
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk",
      "tools": ["latexmk"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-aux-directory=./out-%DOCFILE%",
        "-output-directory=./",
        "%DOC%"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-aux-directory=./out-%DOCFILE%",
        "-output-directory=./",
        "%DOC%"
      ]
    }
  ],
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "workbench.colorTheme": "SynthWave '84",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "terminal.ansiGreen": "#7A942E",
    "terminal.ansiBlue": "#1B294B",
    "terminal.ansiBrightGreen": "#6B9362",
    "terminal.ansiBrightBlue": "#5D8CAE",
    "terminal.foreground": "#FCC9B9",
    "terminalCursor.foreground": "#A4345D",
    "terminal.selectionBackground": "#FFB61E",
    "terminal.ansiRed": "#DB5A6B"
  },
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "workbench.startupEditor": "none",
  "git.ignoreMissingGitWarning": true,
  "workbench.sideBar.location": "right",
  "editor.wordWrap": "on",
  "terminal.integrated.fontFamily": "caskaydiaCove Nerd Font"
}
